server {
  listen   80;
  server_name  <%= node['hostname'] %>;

  location / {	
    proxy_pass http://127.0.0.1:8080;
    
    proxy_set_header	X-App-Lang	"hu";
    
  }

}

server {
  listen   80;
  server_name  sublimail.info;

  location / {	
    proxy_pass http://127.0.0.1:8080;
    
    proxy_set_header	X-App-Lang	"en";
    
  }
}